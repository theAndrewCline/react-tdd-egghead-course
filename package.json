{
	"name": "react-tdd-egghead-course",
	"version": "0.1.0",
	"description": "learning TDD for React",
	"main": "index.js",
	"scripts": {
		"start": "parcel src/index.html",
		"test": "jest --watch"
	},
	"jest": {
		"testURL": "http://localhost",
		"setupFiles": [
			"<rootDir>/src/tests/setupTests.js"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
			"\\.(css|less)$": "<rootDir>/src/tests/__mocks__/styleMock.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/theAndrewCline/react-tdd-egghead-course.git"
	},
	"keywords": [
		"React",
		"TDD"
	],
	"author": "Andrew Cline",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/theAndrewCline/react-tdd-egghead-course/issues"
	},
	"homepage": "https://github.com/theAndrewCline/react-tdd-egghead-course#readme",
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-jest": "^23.4.2",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"enzyme": "^3.4.1",
		"enzyme-adapter-react-16": "^1.2.0",
		"jest": "^23.4.2",
		"parcel-bundler": "^1.9.7"
	},
	"dependencies": {
		"react": "^16.4.2",
		"react-dom": "^16.4.2"
	}
}
